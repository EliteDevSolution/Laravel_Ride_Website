{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "anlutro/l4-settings": "^0.4.9",
        "barryvdh/laravel-debugbar": "^2.4",
        "barryvdh/laravel-translation-manager": "^0.2.7",
        "hesto/multi-auth": "^1.0",
        "laravel/framework": "5.3.*",
        "laravel/passport": "^1.0",
        "laravel/socialite": "^2.0",
        "stripe/stripe-php": "^4.4",
        "symfony/polyfill-iconv": "^1.11",
        "turbo124/laravel-push-notification": "dev-laravel5",
        "tymon/jwt-auth": "0.5.*"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Helper/ControllerHelper.php",
            "app/Helper/ViewHelper.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
